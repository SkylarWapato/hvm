{
  "name": "@skywap/hvm",
  "description": "Hasura version manager.",
  "version": "0.1.3",
  "author": "Skylar Wapato @SkylarWapato",
  "bin": {
    "hvm": "./bin/run"
  },
  "bugs": "https://github.com/SkylarWapato/hvm/issues",
  "dependencies": {
    "@oclif/command": "^1",
    "@oclif/config": "^1",
    "@oclif/plugin-help": "^3",
    "@types/shelljs": "^0.8.8",
    "node-fetch": "^2.6.1",
    "shelljs": "^0.8.4",
    "tslib": "^1"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1",
    "@types/node": "^10",
    "@types/node-fetch": "^2.5.7",
    "globby": "^10",
    "oclif": "^1.16.1",
    "ts-node": "^8",
    "typescript": "^3.3"
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/SkylarWapato/hvm",
  "keywords": [
    "oclif",
    "hasura",
    "graphql"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "hvm",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": "SkylarWapato/hvm",
  "scripts": {
    "postpack": "rm -f oclif.manifest.json",
    "prepack": "rm -rf lib && tsc -b && oclif-dev manifest && oclif-dev readme",
    "test": "echo NO TESTS",
    "version": "oclif-dev readme && git add README.md",
    "pub": "tsc && yarn publish --access public"
  },
  "types": "lib/index.d.ts"
}
